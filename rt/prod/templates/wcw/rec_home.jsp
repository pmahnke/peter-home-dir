<script>
		<%
        String  alertOp = (String) session.getAttribute("alertop");

		if(alertOp != null && !alertOp.equals("")) {
			session.removeAttribute("alertop");
            if (alertOp.equals(Integer.toString(AlertOperation.SHOW_DELIVERY_PREFERENCES))) {
		%>
			    openDeliveryPreferences('<%= response.encodeURL("/Alert?alertop=" + alertOp) %>');
        <%  } else { %>
			    openAlertFromHomePage('<%= response.encodeURL("/Alert?alertop=" + alertOp) %>');
        <%  } %>

		<% } %>

		<%
        String  docURL = (String) session.getAttribute("doc");

		if (docURL != null && !docURL.equals("")) {
			session.removeAttribute("docURL");
		%>
			openAlertFromHomePage('<%= response.encodeURL(docURL) %>');
		<% } %>
</script>

